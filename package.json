{
  "private": true,
  "name": "pathicles",
  "description": "monorepo pathicles",
  "license": "EUPL 1.2",
  "author": "drkrthj <post@dirk-rathje.de>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pathicles/pathicles.git"
  },
  "bugs": {
    "url": "https://github.com/pathicles/pathicles/issues"
  },
  "workspaces": {
    "nohoist": [],
    "packages": [
      "packages/@pathicles/core",
      "packages/@pathicles/config",
      "packages/@pathicles/viewer",
      "packages/@pathicles/vue"
    ]
  },
  "scripts": {
    "build:pathicles": "lerna run --parallel --scope @pathicles/core  --scope @pathicles/viewer --scope @pathicles/config --scope @pathicles/vue build",
    "clean": "lerna exec -- rm -rf node_modules && rm -rf node_modules",
    "dev:pathicles": "lerna run --parallel --scope @pathicles/core  --scope @pathicles/viewer --scope @pathicles/config --scope @pathicles/vue dev",
    "devw:pathicles": "yarn wsrun --parallel dev",
    "lint-fix": "yarn workspaces run lint-fix",
    "prettier-packages": "prettier-package-json --write; lerna exec -- prettier-package-json --write",
    "version:bump-minor": "lerna version minor --no-push --conventional-commits",
    "version:bump-patch": "lerna version patch --no-push --conventional-commits",
    "version:changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 -u && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "optionalDependencies": {
    "lighthouse": "^5.6.0",
    "puppeteer": "^2.1.1"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "d3": "^5.15.0",
    "debug": "^4.1.1",
    "loglevel": "^1.6.7",
    "regl": "^1.4.2",
    "tornis": "^0.3.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.1.3",
    "@vue/cli": "^4.3.0",
    "@vuepress/plugin-register-components": "^1.3.1",
    "@vuepress/shared-utils": "^1.3.1",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "conventional-changelog-cli": "2.0.31",
    "css-loader": "^3.4.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "csv-append": "^1.0.0",
    "d3-array": "^2.4.0",
    "d3-dsv": "^1.2.0",
    "dotenv": "^8.2.0",
    "easy-pdf-merge": "^0.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "^6.2.1",
    "flatted": "^2.0.1",
    "fs-extra-plus": "0.5.15",
    "inquirer": "7.0.4",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "lerna": "^3.20.2",
    "lint-staged": "9.5.0",
    "minimist": "^1.2.0",
    "pkg-up": "^3.1.0",
    "prettier": "1.19.1",
    "prettier-package-json": "^2.1.3",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^4.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-eslint-bundle": "^5.0.2",
    "rollup-plugin-glslify": "^1.1.3",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-require-context": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "2.14.0",
    "typedoc": "^0.16.10",
    "typescript": "3.7.5",
    "upath": "^1.2.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.3.1",
    "vuepress-types": "^0.9.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.5.2",
    "winston": "^3.2.1",
    "wsrun": "^5.2.0"
  }
}
